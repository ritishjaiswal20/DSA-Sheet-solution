#include <iostream>
#include<bits/stdc++.h>
using namespace std;
void merge(vector<int>v1, vector<int>v2)
{
    int s=0,e=0;
    int j=0;
    for(int i=0;i<v1.size();i++)
    {
      if(v1[i]>v2[j])
      {
          swap(v1[i],v2[j]);
          sort(v2.begin(),v2.end());
      }
    } 
   
   for(int i=0;i<v1.size();i++)
   {
       cout<<v1[i]<<" ";
   }
   cout<<endl;
   for(int j=0;j<v2.size();j++)
   {
       cout<<v2[j]<<" ";
   }
}
int main()
{

   vector<int> v1;
   vector<int> v2;
   v1={1,4,7,8,10};
   v2={2,3,9};
   merge(v1,v2);
    return 0;
}
